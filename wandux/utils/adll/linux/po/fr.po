#: src/option.c:33
#, c-format
msgid ""
"%s %s\n"
"Usage: %s [OPTION]... [FILES]...\n"
"\n"
" OPTIONS :\n"
"  -q, --quiet                  Quiet mode\n"
"  -t, --text                   Text mode\n"
"  -v, --version                Display the current version and exit\n"
"  -h, --help                   Display this help and exit\n"
"\n"
"FILES :\n"
"  filea.xml filen.xml...       XML files to parse\n"
msgstr ""
"%s %s\n"
"Usage: %s [OPTION]... [FILES]...\n"
"\n"
" OPTIONS :\n"
"  -q, --quiet                  Mode silencieux\n"
"  -t, --text                   Mode texte\n"
"  -v, --version                Affiche le numéro de version\n"
"  -h, --help                   Affiche ce message d'aide\n"
"\n"
"FILES :\n"
"  filea.xml filen.xml...       Fichiers XML à parser\n"

