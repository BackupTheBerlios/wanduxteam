<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="pfe.migration.server.ejb.bdd">
	<class
		name="GlobalConf"
		table="global_conf"
	>
		<id
			name="Key"
			type="integer"
			column="key"
		>
			<generator class="vm"/>
		</id>

		<property
			name="Hostname"
			column="hostname"
			type="string"
			not-null="true"
			length="50"
		/>


	</class>
</hibernate-mapping>
