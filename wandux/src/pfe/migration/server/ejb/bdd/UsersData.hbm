<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="pfe.migration.server.ejb.bdd">
	<class
		name="UsersData"
		table="users_data"
	>
		<id
			name="Key"
			type="integer"
			column="key"
		>
			<generator class="vm"/>
		</id>

		<property
			name="Type"
			column="type"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="Login"
			column="login"
			type="string"
			not-null="true"
			length="50"
		/>


	</class>
</hibernate-mapping>
